.fab-button-box {
  position: fixed;
  bottom: 150rpx;
  right: 30rpx;
  text-align: center;
}

.fab-button-main {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  top: 40rpx;
  width: 100rpx;
  height: auto;
}

.fab-button-img {
  margin-top: 20rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
}

.fab-button-title {
  display: block;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
}
